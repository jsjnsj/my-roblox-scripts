-- ÂäüËÉΩ3: ËÆæÁΩÆÂèÇÊï∞
tabs["Â∏ÖÈ£û"]:AddTextbox({
    Name = "ÂÜ≤ÊíûÈÄüÂ∫¶",
    Default = "150",
    PlaceholderText = "ËæìÂÖ•ÂÜ≤ÊíûÈÄüÂ∫¶Ôºà50-300Ôºâ",
    Callback = function(speedText)
        if speedText ~= "" then
            local speed = tonumber(speedText)
            if speed and speed >= 50 and speed <= 300 then
                _G.CollisionSpeed = speed
                OrionLib:MakeNotification({
                    Name = "ÂÜ≤ÊíûÈÄüÂ∫¶",
                    Content = "ÂÜ≤ÊíûÈÄüÂ∫¶Â∑≤ËÆæÁΩÆ‰∏∫: " .. speed,
                    Time = 3
                })
            end
        end
    end
})


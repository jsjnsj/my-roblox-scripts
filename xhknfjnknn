-- ÂÆûÊó∂Êó∂Èó¥Êõ¥Êñ∞
local function UpdateTime()
    while wait(1) do
        pcall(function()
            local currentTime = GetBeijingTime()
            timeLabel:Set("üïê Âåó‰∫¨Êó∂Èó¥: " .. currentTime)
        end)
    end
end

-- ÂàùÂßãÂåñÊó∂Êõ¥Êñ∞‰ø°ÊÅØ
UpdatePlayerInfo()
coroutine.wrap(UpdateTime)()


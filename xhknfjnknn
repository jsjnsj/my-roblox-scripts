-- ÊóãËΩ¨ÂºÄÂÖ≥
tabs["ÊóãËΩ¨"]:AddToggle({
    Name = "üí´ ÂºÄÂêØ/ÂÖ≥Èó≠ÊóãËΩ¨",
    Default = false,
    Callback = function(v)
        if v then
            local character = game.Players.LocalPlayer.Character
            if character then
                local rootPart = character:FindFirstChild("HumanoidRootPart")
                if rootPart then
                    if _G.SpinEffect then
                        _G.SpinEffect:Destroy()
                    end
                    
                    local spin = Instance.new("BodyAngularVelocity")
                    spin.AngularVelocity = Vector3.new(0, _G.CurrentSpinSpeed or 20, 0)
                    spin.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
                    spin.Parent = rootPart
                    _G.SpinEffect = spin
                    
                    OrionLib:MakeNotification({
                        Name = "ÊóãËΩ¨ÂºÄÂêØ",
                        Content = "ÊóãËΩ¨Â∑≤ÂºÄÂêØÔºåÈÄüÂ∫¶: " .. (_G.CurrentSpinSpeed or 20),
                        Time = 3
                    })
                end
            end
        else
            if _G.SpinEffect then
                _G.SpinEffect:Destroy()
                _G.SpinEffect = nil
                OrionLib:MakeNotification({
                    Name = "ÊóãËΩ¨ÂÖ≥Èó≠",
                    Content = "ÊóãËΩ¨Â∑≤ÂÖ≥Èó≠",
                    Time = 2
                })
            end
        end
    end
})

-- Âø´ÈÄüÈ¢ÑËÆæÊåâÈíÆ

-- Ê∑ªÂä†Âø´Êç∑ÈîÆ
local toggleConnection
toggleConnection = game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.F9 then
        MainWindow:ToggleUI()
    end
end)

table.insert(_G.UniversalUITool.Connections, toggleConnection)

OrionLib:MakeNotification({
    Name = "xhknfjnknnËÑöÊú¨Âä†ËΩΩÊàêÂäü",
    Content = "Ê¨¢ËøéÊÇ®‰ΩøÁî®Á•ù‰Ω†Áé©ÁöÑÂºÄÂøÉü•∞",
    Time = 4
})

print("=== xhknfjnknnËÑöÊú¨Âä†ËΩΩÊàêÂäü ===")


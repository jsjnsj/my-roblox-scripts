tabs["Áé©ÂÆ∂‰ø°ÊÅØ"]:AddLabel("=== Âü∫Êú¨‰ø°ÊÅØ ===")

-- Ëé∑ÂèñÂåó‰∫¨Êó∂Èó¥ÂáΩÊï∞
local function GetBeijingTime()
    local success, result = pcall(function()
        local timeService = game:GetService("HttpService")
        local response = timeService:JSONDecode(game:HttpGet("http://worldtimeapi.org/api/timezone/Asia/Shanghai"))
        return response.datetime
    end)
    
    if success then
        return os.date("%Y-%m-%d %H:%M:%S", os.time())
    else
        return os.date("%Y-%m-%d %H:%M:%S", os.time())
    end
end

-- Ê£ÄÊµãÊ≥®ÂÖ•Âô®Á±ªÂûã
local function GetExecutorInfo()
    if pcall(function() return syn and syn.request end) then
        return "Synapse X"
    elseif pcall(function() return crypt and crypt.base64_decode end) then
        return "Krnl"
    elseif pcall(function() return is_sirhurt_closure end) or pcall(function() return sirhurt end) then
        return "SirHurt"
    elseif pcall(function() return secure_load end) then
        return "Script Ware"
    elseif pcall(function() return isfluxus end) then
        return "Fluxus"
    elseif pcall(function() return getexecutorname end) then
        local name = getexecutorname()
        return name or "Êú™Áü•Ê≥®ÂÖ•Âô®"
    else
        if pcall(function() return identifyexecutor end) then
            local exec = identifyexecutor()
            return exec or "Roblox Lua"
        else
            return "Roblox Lua"
        end
    end
end

-- ÂàõÂª∫‰ø°ÊÅØÊòæÁ§∫Ê†áÁ≠æ
local playerNameLabel = tabs["Áé©ÂÆ∂‰ø°ÊÅØ"]:AddLabel("üë§ Áé©ÂÆ∂ÂêçÁß∞: Âä†ËΩΩ‰∏≠...")
local playerIDLabel = tabs["Áé©ÂÆ∂‰ø°ÊÅØ"]:AddLabel("üî¢ Áî®Êà∑ID: Âä†ËΩΩ‰∏≠...")
local executorLabel = tabs["Áé©ÂÆ∂‰ø°ÊÅØ"]:AddLabel("‚ö° Ê≥®ÂÖ•Âô®: Âä†ËΩΩ‰∏≠...")
local timeLabel = tabs["Áé©ÂÆ∂‰ø°ÊÅØ"]:AddLabel("üïê Âåó‰∫¨Êó∂Èó¥: Âä†ËΩΩ‰∏≠...")

-- Êõ¥Êñ∞Áé©ÂÆ∂‰ø°ÊÅØÂáΩÊï∞
local function UpdatePlayerInfo()
    local localPlayer = game.Players.LocalPlayer
    local executorInfo = GetExecutorInfo()
    local beijingTime = GetBeijingTime()
    
    playerNameLabel:Set("üë§ Áé©ÂÆ∂ÂêçÁß∞: " .. localPlayer.Name)
    playerIDLabel:Set("üî¢ Áî®Êà∑ID: " .. localPlayer.UserId)
    executorLabel:Set("‚ö° Ê≥®ÂÖ•Âô®: " .. executorInfo)
    timeLabel:Set("üïê Âåó‰∫¨Êó∂Èó¥: " .. beijingTime)
end


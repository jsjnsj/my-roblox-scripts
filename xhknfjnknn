-- ÂÆûÊó∂Êõ¥Êñ∞ÊòæÁ§∫
spawn(function()
    while wait(1) do
        local character = game.Players.LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                healthLabel:Set("ÂΩìÂâç: " .. math.floor(humanoid.Health) .. " / ‰∏äÈôê: " .. math.floor(humanoid.MaxHealth))
            end
        end
    end
end)

-- ÂàùÂßãÂåñÂÖ®Â±ÄÂèòÈáè
_G.CollisionSpeed = 150
_G.LaunchPower = 250
_G.ChargeDistance = 5
_G.CurrentSpinSpeed = 20
_G.JumpHeight = 50

-- Êó†ÈôêË∑≥Ë∑ÉÂäüËÉΩ
local InfiniteJumpEnabled = false
local JumpConnection

tabs["‰∏ªÂäüËÉΩ"]:AddToggle({
    Name = "Êó†ÈôêË∑≥Ë∑É",
    Default = false,
    Callback = function(v)
        if v then
            InfiniteJumpEnabled = true
            JumpConnection = game:GetService("UserInputService").JumpRequest:Connect(function()
                if InfiniteJumpEnabled then
                    local character = game.Players.LocalPlayer.Character
                    if character and character:FindFirstChildOfClass("Humanoid") then
                        character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    end
                end
            end)
        else
            InfiniteJumpEnabled = false
            if JumpConnection then
                JumpConnection:Disconnect()
            end
        end
    end
})

-- ÁßªÂä®ÈÄüÂ∫¶ËÆæÁΩÆ
tabs["‰∏ªÂäüËÉΩ"]:AddTextbox({
    Name = "ËÆæÁΩÆÁßªÂä®ÈÄüÂ∫¶",
    Default = "",
    PlaceholderText = "ËæìÂÖ•ÈÄüÂ∫¶ÂÄºÔºà16-200Ôºâ",
    Callback = function(speedText)
        if speedText ~= "" then
            local speed = tonumber(speedText)
            if speed and speed >= 16 and speed <= 200 then
                local char = game.Players.LocalPlayer.Character
                if char and char:FindFirstChildOfClass("Humanoid") then
                    char.Humanoid.WalkSpeed = speed
                end
            end
        end
    end
})

-- Ë∑≥Ë∑ÉÈ´òÂ∫¶ËÆæÁΩÆ
tabs["‰∏ªÂäüËÉΩ"]:AddTextbox({
    Name = "ËÆæÁΩÆË∑≥Ë∑ÉÈ´òÂ∫¶",
    Default = "",
    PlaceholderText = "ËæìÂÖ•Ë∑≥Ë∑ÉÈ´òÂ∫¶Ôºà1-200Ôºâ",
    Callback = function(jumpText)
        if jumpText ~= "" then
            local jumpHeight = tonumber(jumpText)
            if jumpHeight and jumpHeight >= 1 and jumpHeight <= 200 then
                _G.JumpHeight = jumpHeight
                local char = game.Players.LocalPlayer.Character
                if char and char:FindFirstChildOfClass("Humanoid") then
                    char.Humanoid.JumpPower = jumpHeight
                end
                OrionLib:MakeNotification({
                    Name = "Ë∑≥Ë∑ÉÈ´òÂ∫¶",
                    Content = "Ë∑≥Ë∑ÉÈ´òÂ∫¶Â∑≤ËÆæÁΩÆ‰∏∫: " .. jumpHeight,
                    Time = 3
                })
            end
        end
    end
})

-- Ë∑≥Ë∑ÉÊåâÈíÆ
tabs["‰∏ªÂäüËÉΩ"]:AddButton({
    Name = "ü¶ò Ë∂ÖÁ∫ßË∑≥Ë∑É",
    Callback = function()
        local character = game.Players.LocalPlayer.Character
        if character and character:FindFirstChildOfClass("Humanoid") then
            local humanoid = character.Humanoid
            local rootPart = character:FindFirstChild("HumanoidRootPart")
            
            if rootPart then
                -- ‰øùÂ≠òÂéüÂßãË∑≥Ë∑ÉÈ´òÂ∫¶
                local originalJumpPower = humanoid.JumpPower
                
                -- ËÆæÁΩÆËá™ÂÆö‰πâË∑≥Ë∑ÉÈ´òÂ∫¶
                humanoid.JumpPower = _G.JumpHeight or 50
                
                -- ÊâßË°åË∑≥Ë∑É
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                
                -- Ê∑ªÂä†Âêë‰∏äÁöÑÂäõÊù•Â¢ûÂä†Ë∑≥Ë∑ÉÈ´òÂ∫¶
                local bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = Vector3.new(0, (_G.JumpHeight or 50) * 2, 0)
                bodyVelocity.MaxForce = Vector3.new(0, math.huge, 0)
                bodyVelocity.Parent = rootPart
                
                -- 2ÁßíÂêéÁßªÈô§Êé®Âäõ
                wait(2)
                if bodyVelocity and bodyVelocity.Parent then
                    bodyVelocity:Destroy()
                end
                
                -- ÊÅ¢Â§çÂéüÂßãË∑≥Ë∑ÉÈ´òÂ∫¶
                humanoid.JumpPower = originalJumpPower
                
                OrionLib:MakeNotification({
                    Name = "Ë∂ÖÁ∫ßË∑≥Ë∑É",
                    Content = "Ë∑≥Ë∑ÉÈ´òÂ∫¶: " .. (_G.JumpHeight or 50),
                    Time = 3
                })
            end
        end
    end
})

-- Â§úËßÜÂäüËÉΩ
tabs["‰∏ªÂäüËÉΩ"]:AddToggle({
    Name = "Â§úËßÜÊ®°Âºè",
    Default = false,
    Callback = function(v)
        if v then
            game.Lighting.Brightness = 2
            game.Lighting.Ambient = Color3.new(1, 1, 1)
            game.Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
        else
            game.Lighting.Brightness = 1
            game.Lighting.Ambient = Color3.new(0.5, 0.5, 0.5)
            game.Lighting.OutdoorAmbient = Color3.new(0.5, 0.5, 0.5)
        end
    end
})

-- Á©øÂ¢ôÂäüËÉΩ
local NoclipEnabled = false
local NoclipConnection

tabs["‰∏ªÂäüËÉΩ"]:AddToggle({
    Name = "Á©øÂ¢ôÊ®°Âºè",
    Default = false,
    Callback = function(v)
        if v then
            NoclipEnabled = true
            local character = game.Players.LocalPlayer.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
            
            NoclipConnection = game:GetService("RunService").Stepped:Connect(function()
                if NoclipEnabled and game.Players.LocalPlayer.Character then
                    for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                else
                    if NoclipConnection then
                        NoclipConnection:Disconnect()
                    end
                end
            end)
        else
            NoclipEnabled = false
            if NoclipConnection then
                NoclipConnection:Disconnect()
            end
        end
    end
})

-- Âº∫Â§ßÁöÑÈò≤Áî©È£ûÂäüËÉΩ
local AntiLaunchEnabled = false
local AntiLaunchHeartbeat

local function EnableAdvancedAntiLaunch()
    if AntiLaunchEnabled then
        AntiLaunchHeartbeat = game:GetService("RunService").Heartbeat:Connect(function()
            local character = game.Players.LocalPlayer.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                local rootPart = character.HumanoidRootPart
                local humanoid = character:FindFirstChildOfClass("Humanoid")
                
                for _, obj in pairs(rootPart:GetChildren()) do
                    if obj:IsA("BodyVelocity") or obj:IsA("BodyForce") or obj:IsA("BodyThrust") or 
                       obj:IsA("BodyAngularVelocity") or obj:IsA("BodyPosition") then
                        if obj.Name ~= "ChargeVelocity" and obj.Name ~= "SpinEffect" and obj.Name ~= "AntiLaunchAnchor" then
                            pcall(function() obj:Destroy() end)
                        end
                    end
                end
                
                if rootPart.AssemblyLinearVelocity.Magnitude > 50 then
                    pcall(function()
                        rootPart.Velocity = Vector3.new(0, 0, 0)
                        rootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
                    end)
                end
                
                if rootPart.AssemblyAngularVelocity.Magnitude > 20 then
                    pcall(function()
                        rootPart.RotVelocity = Vector3.new(0, 0, 0)
                        rootPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
                    end)
                end
                
                if not rootPart:FindFirstChild("AntiLaunchAnchor") then
                    local anchor = Instance.new("BodyPosition")
                    anchor.Name = "AntiLaunchAnchor"
                    anchor.P = 10000
                    anchor.D = 1000
                    anchor.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                    anchor.Position = rootPart.Position
                    anchor.Parent = rootPart
                else
                    local anchor = rootPart:FindFirstChild("AntiLaunchAnchor")
                    if anchor then
                        anchor.Position = rootPart.Position
                    end
                end
                
                if rootPart.Position.Y > 500 then
                    pcall(function()
                        local safeCFrame = CFrame.new(rootPart.Position.X, 50, rootPart.Position.Z)
                        rootPart.CFrame = safeCFrame
                    end)
                end
                
                if humanoid then
                    humanoid.PlatformStand = false
                    if humanoid:GetState() == Enum.HumanoidStateType.FallingDown or 
                       humanoid:GetState() == Enum.HumanoidStateType.Ragdoll then
                        humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
                    end
                end
            end
        end)
        
        local function monitorNetworkOwnership()
            while AntiLaunchEnabled and wait(1) do
                pcall(function()
                    local character = game.Players.LocalPlayer.Character
                    if character and character:FindFirstChild("HumanoidRootPart") then
                        local rootPart = character.HumanoidRootPart
                        rootPart:SetNetworkOwner(game.Players.LocalPlayer)
                    end
                end)
            end
        end
        
        coroutine.wrap(monitorNetworkOwnership)()
    end
end

tabs["‰∏ªÂäüËÉΩ"]:AddToggle({
    Name = "üõ°Ô∏è Ë∂ÖÁ∫ßÈò≤Áî©È£û‰øùÊä§",
    Default = false,
    Callback = function(v)
        AntiLaunchEnabled = v
        if v then
            OrionLib:MakeNotification({
                Name = "Ë∂ÖÁ∫ßÈò≤Êä§",
                Content = "Ë∂ÖÁ∫ßÈò≤Áî©È£û‰øùÊä§Â∑≤ÂºÄÂêØÔºÅÁé∞Âú®Êó†‰∫∫ËÉΩÁî©È£û‰Ω†ÔºÅ",
                Time = 4
            })
            EnableAdvancedAntiLaunch()
        else
            if AntiLaunchHeartbeat then
                AntiLaunchHeartbeat:Disconnect()
                AntiLaunchHeartbeat = nil
            end
            
            pcall(function()
                local character = game.Players.LocalPlayer.Character
                if character and character:FindFirstChild("HumanoidRootPart") then
                    local rootPart = character.HumanoidRootPart
                    local anchor = rootPart:FindFirstChild("AntiLaunchAnchor")
                    if anchor then
                        anchor:Destroy()
                    end
                end
            end)
            
            OrionLib:MakeNotification({
                Name = "Ë∂ÖÁ∫ßÈò≤Êä§",
                Content = "Èò≤Áî©È£û‰øùÊä§Â∑≤ÂÖ≥Èó≠",
                Time = 3
            })
        end
    end
})


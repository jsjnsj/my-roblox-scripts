    Name = "ËÆæÁΩÆÂΩìÂâçÁîüÂëΩÂÄº (1-200)",
    Default = "100",
    TextDisappear = false,
    Callback = function(Value)
        local newHealth = tonumber(Value)
        if newHealth and newHealth >= 1 and newHealth <= 200 then
            local character = game.Players.LocalPlayer.Character
            if character then
                local humanoid = character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.Health = newHealth
                    OrionLib:MakeNotification({
                        Name = "ÁîüÂëΩÂÄºËÆæÁΩÆ",
                        Content = "ÂΩìÂâçÁîüÂëΩÂÄºÂ∑≤ËÆæÁΩÆ‰∏∫: " .. newHealth,
                        Time = 2
                    })
                end
            end
        else
            OrionLib:MakeNotification({
                Name = "ÈîôËØØ",
                Content = "ËØ∑ËæìÂÖ•1-200‰πãÈó¥ÁöÑÊï∞Â≠ó",
                Time = 2
            })
        end
    end
})

